{
    "extends": ["eslint:recommended", "plugin:@typescript-eslint/recommended"],
    "ignorePatterns": ["**/*.js"],
    "plugins": ["@typescript-eslint"],
    "root": true,
    "parser": "@typescript-eslint/parser",
    "rules": {
        "max-len": ["error", 150],
        "@typescript-eslint/no-empty-interface": "off"
    }
}
